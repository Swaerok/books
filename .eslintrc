{
    "extends": [
        "airbnb-base",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
      "react/display-name": 0,
        "no-tabs": "off",
        "no-plusplus": "off",
        "class-methods-use-this": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "semi": "off",
        "indent": ["error", "tab"],
        "global-require": "off",
        "import/prefer-default-export": "off",
        "arrow-body-style": "off"
    }
  }